import java.util.*;
public class pn{
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        int n = sc.nextInt();
        if (n % 2 == 0) {
            System.out.println("Even");
            
        }
        else if(n==0){
            System.out.println("Zero");
        }
        else{
            System.out.println("odd");
        }
        


        
    }
