import java.util.*;
public class pn{
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        int x = sc.nextInt();
        if(x % 400==0){
            System.out.println("not a leap year");
        }
        else if(x % 4==0 && x%100!=0){
            System.out.println("leap year");
        }
        
    }
}
